{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react-hooks/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "root": true,
  "rules": {
    "no-console": ["warn", { "allow": ["info", "error"] }],
    "sort-imports": "warn",
    "@typescript-eslint/no-empty-function": "off"
  }
}
